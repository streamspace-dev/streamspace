          "fromAddress": {
            "type": "string",
            "title": "From Email Address",
            "format": "email"
          },
          "toAddresses": {
            "type": "array",
            "title": "To Email Addresses",
            "items": {"type": "string", "format": "email"}
          },
          "notifyOnSessionCreated": {
            "type": "boolean",
